class Solution {
    public long countVowels(String word) {
        int i,j,count=0;
        long total=0;
        for(i=0;i<word.length();i++)
        {
            if(word.charAt(i)=='a' || word.charAt(i)=='e' || word.charAt(i)=='o' || word.charAt(i)=='i'|| word.charAt(i)=='u')
            {
                total+=(long)(i+1)*(long)(word.length()-i);

            }
        }
       return total;
    }
}

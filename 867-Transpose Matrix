class Solution {
    public int[][] transpose(int[][] matrix) {
        int i,j;
        int a=matrix.length;
        int b=matrix[0].length;
        int arr2[][]=new int[b][a];
        for(i=0;i<a;i++)
        {
            for(j=0;j<b;j++)
            {
                arr2[j][i]=matrix[i][j];
            }
        }
        return arr2;
    }
}
